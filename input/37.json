    "scripts": {
        "start:dev": "cross-env NODE_ENV=dev webpack && cross-env NODE_ENV=dev NODE_TLS_REJECT_UNAUTHORIZED=0 pm2 startOrReload process.json",
        "start:prod": "cross-env NODE_ENV=production webpack && cross-env NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 pm2 startOrReload process.json",
        "start:local": "cross-env NODE_ENV=local NODE_TLS_REJECT_UNAUTHORIZED=0 node app.js",
        "start": "cross-env NODE_ENV=${cross-env NODE_ENV:=production}",
        "build": "cross-env NODE_ENV=local ./node_modules/.bin/webpack",
        "check": "npm-check -E -u -s --no-emoji",
        "postversion": "/bin/bash sonar-version",
        "test": "jest --verbose"
    },
